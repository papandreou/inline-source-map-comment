{
  "name": "inline-source-map-comment",
  "version": "1.0.1",
  "description": "Create an inline source map comment from a source map",
  "repository": "shinnn/inline-source-map-comment",
  "author": {
    "name": "Shinnosuke Watanabe",
    "url": "https://github.com/shinnn"
  },
  "scripts": {
    "pretest": "jscs . && eslint --ignore-path .gitignore .",
    "test-node": "node test-api/test-node.js | tap-spec",
    "test-browser": "testling | tap-spec",
    "test-cli": "node test-cli/test-cli.js | tap-spec",
    "test": "npm run test-node && npm run test-browser && npm run test-cli",
    "coverage": "istanbul cover test-api/test-node.js",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/shinnn/inline-source-map-comment/blob/master/LICENSE"
    }
  ],
  "main": "inline-source-map-comment-node.js",
  "browser": "inline-source-map-comment-cjs.js",
  "bin": "cli.js",
  "files": [
    "cli.js",
    "inline-source-map-comment-cjs.js",
    "inline-source-map-comment-node.js",
    "LICENSE"
  ],
  "keywords": [
    "inline",
    "sourcemap",
    "source",
    "map",
    "comment",
    "js",
    "css",
    "cli",
    "browser",
    "client-side"
  ],
  "dependencies": {
    "chalk": "^0.5.1",
    "get-stdin": "^3.0.2",
    "minimist": "^1.1.0",
    "sum-up": "^1.0.0",
    "xtend": "^4.0.0"
  },
  "devDependencies": {
    "eslint": "^0.10.2",
    "istanbul": "^0.3.5",
    "istanbul-coveralls": "^1.0.1",
    "jscs": "^1.8.1",
    "tap-spec": "^2.1.1",
    "tape": "^3.0.3",
    "testling": "^1.7.1"
  },
  "jscsConfig": {
    "preset": "google",
    "maximumLineLength": null,
    "validateLineBreaks": "LF",
    "excludeFiles": [
      "{coverage,node_modules}/**"
    ]
  },
  "testling": {
    "files": [
      "test-api/test-browser.js",
      "test-api/test-browser-cjs.js"
    ]
  }
}
